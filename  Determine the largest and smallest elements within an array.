#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int arr[n];
    
    // input
    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    int max = arr[0];
    int min = arr[0];

    // find largest & smallest
    for(int i = 1; i < n; i++) {
        if(arr[i] > max) {
            max = arr[i];
        }
        if(arr[i] < min) {
            min = arr[i];
        }
    }

    printf("Largest = %d\n", max);
    printf("Smallest = %d\n", min);

    return 0;
}