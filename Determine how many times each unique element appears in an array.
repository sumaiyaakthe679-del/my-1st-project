#include <stdio.h>

int main() {
    int a[] = {1, 2, 4, 2, 1, 4, 4, 5};
    int n = 8;
    int freq[100] = {0};

    for(int i = 0; i < n; i++) {
        freq[a[i]]++;
    }

    for(int i = 0; i < 100; i++) {
        if(freq[i] > 0) {
            printf("%d appears %d times\n", i, freq[i]);
        }
    }

    return 0;
}