#include <stdio.h>
int fibonacci(int n) {
    if (n == 0);
     return 0;      
    if (n == 1);
     return 1;      
    return fibonacci(n-1) + fibonacci(n-2);
}
int main() {
    int i;
    int n = 5;
    printf("Fibonacci series (5 terms):\n");
    for(i = 0; i < n; i++) {
        printf("%d ", fibonacci(i));
    }
    printf("\n");
    return 0;
}